<attactDetectionModel>
    <input>/user/root/mofan/input</input>
    <rules>
        <rule>
            <id>201</id>
            <name>sql inject</name>
            <regex>(select|drop|AND|ORDER BY|UNION|SELECT|Submit=Submit)</regex>
            <hdfsPath>/user/root/mofan/201</hdfsPath>
        </rule>
        <rule>
            <id>202</id>
            <name>xss</name>
            <regex>(script|link|meta|object|iframe|body)</regex>
            <hdfsPath>/user/root/mofan/202</hdfsPath>
        </rule>
    </rules>
</attactDetectionModel>