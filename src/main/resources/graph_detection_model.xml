<graphDetectionModel>
    <input>/user/root/mofan/test.dat</input>
    <rules>
        <rule>
            <id>101</id>
            <name>phpspy</name>
            <regex>(function)</regex>
            <hdfsPath>/user/root/mofan/101</hdfsPath>
        </rule>
        <!--<rule>-->
            <!--<id>102</id>-->
            <!--<name>knife</name>-->
            <!--<regex>TODO</regex>-->
            <!--<score>1</score>-->
            <!--<hdfsPath>/apps</hdfsPath>-->
        <!--</rule>-->
        <rule>
            <id>103</id>
            <name>filePath</name>
            <regex>(file|upfile|upload|uploadfile)*\.(asp|php|jsp|aspx|phtml|asa|hta|cer)</regex>
            <hdfsPath>/user/root/mofan/103</hdfsPath>
        </rule>
        <rule>
            <id>104</id>
            <name>iis6</name>
            <regex>(asp/|asp;)</regex>
            <hdfsPath>/user/root/mofan/104</hdfsPath>
        </rule>
        <rule>
            <id>105</id>
            <name>apache</name>
            <regex>(\.rar)</regex>
            <hdfsPath>/user/root/mofan/105</hdfsPath>
        </rule>
        <rule>
            <id>106</id>
            <name>nginx</name>
            <regex>(\.jsp)*\.php</regex>
            <hdfsPath>/user/root/mofan/106</hdfsPath>
        </rule>
    </rules>
</graphDetectionModel>